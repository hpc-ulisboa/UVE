###############################################################################
# Rules file
# - - - - - - - - -
# Jo√£o Domingos
#
# YAML notation, json is also valid
# Each ID is met with the template values
# The key (<ID_k>) and value <ID_v> for each element is subst in the template

# width replacement rule
width:
    - b : 0
    - h : 1
    - w : 2
    - d : 3

widthb:
    - h : 1
    - w : 2
    - d : 3

widthh:
    - b : 0
    - w : 2
    - d : 3

widthw:
    - b : 0
    - h : 1
    - d : 3

widthd:
    - b : 0
    - h : 1
    - w : 2

s:
    - "" : 0
    - s : 1

cw:
    - "" : 0
    - cw : 1

fps:
    - us : 0
    - fp : 1
    - sg : 2

fpo:
    - fp : 1
    - sg : 0

acc:
    - "" : 0
    - acc: 1

stype:
    - app: 0
    - end: 1
    - str: 2
    - smp: 3

tget:
    - siz: 0
    - str: 1
    - ofs: 2

bmod:
    - inc: 0
    - dec: 1
    
bind:
    - inc: 0
    - dec: 1
    - add: 2
    - sub: 3
    - set: 4

sg:
    - "" : 0
    - sg : 1

pmz:
    - "" : 0
    - z  : 1

pmm:
    - "" : 0
    - m  : 1

inds:
    - "" : 0
    - inds  : 1

mem:
    - "" : 0
    - mem1 : 1
    - mem2 : 2
    - mem3 : 3

